{
  "branches": [
    "+([0-9])?(.{+([0-9]),x}).x",
    "main"
  ],
  "release": {
    "branches": ["main"]
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/github",
    ["@semantic-release/exec",
      {
        "publishCmd": "echo ::set-output name=semanticVersion::${nextRelease.version}"
      }
    ]
  ]
}
